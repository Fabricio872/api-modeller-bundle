{
    "name": "fabricio872/api-modeller-bundle",
    "description": "Symfony bundle for translating foreign API to Doctrine-like models",
    "type": "symfony-bundle",
    "keywords": ["library", "symfony", "symfony-bundle", "api", "model"],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^7.4|^8.0",
        "fabricio872/api-modeller": "^0.1",
        "symfony/config": "^4.4|^5",
        "symfony/dependency-injection": "^4.4|^5"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.4|^5",
        "symplify/easy-coding-standard": "^9.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Fabricio872\\ApiModellerBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Fabricio",
            "email": "fabricio872@gmail.com"
        }
    ],
    "scripts": {
        "test-psr": "vendor/bin/ecs check src",
        "fix-psr": "vendor/bin/ecs check src --fix"
    }
}
